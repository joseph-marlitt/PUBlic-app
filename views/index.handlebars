<section class="frontPage bg-primary text-center" id="frontPage">

    <div class="container">
        <div id="status"></div>

        <div class="row">
            <div class="col-md-8 mx-auto">
                <!-- horizontal center -->
                <h2 class="section-heading">PUBlic Transit</h2>
                <h3 class="section-heading">Enjoy local brews and breweries, safely!</h3>
            </div>
        </div>
        <div class="row text-center">

            <div class="col-xs-4">

                <div id="login"></div>

            </div>
        </div>
        <div class="row" id="bottom">
            <div class="col-md-4 mx-auto">

                <div id="controlPanel">
                    <a href="/untappd">
                        <button type="button" class="btn" name="button">Over 21? Pick your favorite beers!</button>
                    </a>
                    <div id="searchType">
                        <a href data-type="search" class="selected"></a>
                    </div>
                    <div id="searchForms">
                        <form action="/" method="post" id="searchForm">
                            <div class="form-group">
                                <label for="brewery-search"> Search for a brewery: </label>
                                <input id="brewery-search" name="brewerySearch" type="text"></input>
                            </div>
                            <!-- close form group -->
                            <input type="submit" class="btn" id="submitButton" value="Find Breweries!">
                    </div>
                    </form>
                </div>
                <!-- close searchForm -->
            </div>
            <!-- close control panel -->
        </div>
        <!-- close col-md-4 -->
    </div>
    <!-- close row -->
    <div class="row">
        <div class="col-md-6 mx-auto animated zoomIn" id="map" libraries="places, directions"></div>
    </div>
    <div class="row">
        <div class="col-md-6 mx-auto" id="warnings-panel" libraries="directions"></div>
    </div>
    <div class="row">
        <div class="col-md-6 mx-auto" id="directions-panel" libraries="directions"></div>
    </div>

</section>

